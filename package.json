{
  "name": "swell-js",
  "version": "3.21.2",
  "description": "Swell JS library for client-side stores",
  "engines": {
    "node": ">=14.19.1",
    "npm": ">=8.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swellstores/swell-js"
  },
  "license": "MIT",
  "main": "./dist/swell.cjs.js",
  "module": "./dist/index.js",
  "browser": "./dist/swell.umd.min.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "deepmerge": "4.2.2",
    "fast-case": "1.7.0",
    "qs": "6.10.3"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.8.0",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.8.3",
    "@babel/runtime": "7.20.1",
    "@material-ui/core": "4.9.0",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.42",
    "@rollup/plugin-commonjs": "22.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-replace": "5.0.1",
    "babel-jest": "29.3.1",
    "babel-loader": "8.0.6",
    "cross-var": "1.1.0",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "jest": "24.8.0",
    "jest-fetch-mock": "3.0.3",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "react": "16.12.0",
    "react-dev-utils": "11.0.3",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "recompose": "0.30.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "replace": "1.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.72.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-polyfill-node": "0.10.2",
    "style-loader": "1.1.3",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.11.2"
  },
  "scripts": {
    "test": "jest --config=jest.config.json",
    "test:watch": "jest --config=jest.config.json --watch",
    "test:page": "webpack-dev-server --open --config test/page/webpack.config.js",
    "watch": "nodemon --watch src --exec 'npm run build'",
    "inject-version": "cross-var replace \"@VERSION@\" $npm_package_version dist/api.js",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "version": "npm run build && git add -A dist",
    "bump": "npm version patch && npm publish",
    "lint": "prettier --write \"{src,types}/**/*.{js,jsx,json,ts}\""
  }
}
