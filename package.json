{
  "name": "swell-js",
  "version": "4.0.5",
  "description": "Swell JS library for client-side stores",
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swellstores/swell-js"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/swell.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/swell.umd.min.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/swell.cjs"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "deepmerge": "^4.3.1",
    "fast-case": "^1.7.0",
    "qs": "^6.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-import-attributes": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.8",
    "@material-ui/core": "4.9.0",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.42",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.5",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.3",
    "cross-var": "^1.1.0",
    "css-loader": "^6.9.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.19.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.4",
    "react": "16.12.0",
    "react-dev-utils": "11.0.3",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "recompose": "0.30.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "rimraf": "^5.0.5",
    "rollup": "3.29.4",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-polyfill-node": "0.13.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "test": "jest --config=jest.config.json --detectOpenHandles",
    "test:watch": "jest --config=jest.config.json --watch",
    "test:page": "webpack-dev-server --open --config test/page/webpack.config.cjs",
    "watch": "nodemon --watch src --exec 'npm run build'",
    "inject-version": "cross-var replace \"@VERSION@\" $npm_package_version dist/api.js",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "bump": "npm version patch && npm publish",
    "prepack": "rimraf dist && npm run build",
    "lint": "prettier --write \"{src,types}/**/*.{js,jsx,json,ts}\""
  }
}
