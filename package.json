{
  "name": "swell-js",
  "version": "3.12.2",
  "description": "Swell JS library for client-side stores",
  "repository": {
    "type": "git",
    "url": "https://github.com/swellstores/swell-js"
  },
  "main": "./dist/api.js",
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "deepmerge": "4.2.2",
    "isomorphic-fetch": "3.0.0",
    "lodash": "4.17.21",
    "object-keys-normalizer": "1.0.1",
    "qs": "6.7.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.8.3",
    "@material-ui/core": "4.9.0",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.42",
    "babel-loader": "8.0.6",
    "cross-var": "1.1.0",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "jest": "24.8.0",
    "jest-fetch-mock": "3.0.3",
    "nodemon": "2.0.2",
    "react": "16.12.0",
    "react-dev-utils": "11.0.3",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "recompose": "0.30.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "replace": "1.2.0",
    "style-loader": "1.1.3",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.11.2"
  },
  "scripts": {
    "test": "jest --config=jest.config.json",
    "test:watch": "jest --config=jest.config.json --watch",
    "test:page": "webpack-dev-server --open --config test/page/webpack.config.js",
    "watch": "nodemon --watch src --exec 'npm run build'",
    "inject-version": "cross-var replace \"@VERSION@\" $npm_package_version dist/api.js",
    "build": "babel src --out-dir dist --ignore src/**/*.test.js && npm run inject-version",
    "version": "npm run build && git add -A dist",
    "bump": "npm version patch && npm publish"
  }
}
